code = "shader_type canvas_item;

uniform float blur;
uniform int iter;
uniform int half;
uniform int itersq;
uniform float modulate;

void fragment() {
	vec4 avg = vec4(0);
	for(int x = -half; x < half + 1; x++) {
		for(int y = -half; y < half + 1; y++) {
			avg += texture(SCREEN_TEXTURE, SCREEN_UV + vec2(float(x) * blur, float(y) * blur));
		}
	}
	COLOR = avg / float(itersq);
	COLOR.rgb *= modulate;
}"

